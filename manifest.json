{

  // Required
  "manifest_version": 2,
  "name": "Read Tab Later",
  "version": "1.0",


  "browser_action": {
    "default_icon": "localhost.png"
  },

  "chrome_url_overrides": {
    "newtab": "index.html"
  },

  "background": {
    "scripts": ["background.js"]
  },

  "permissions": ["tabs", "<all_urls>", "http://fonts.googleapis.com/", "https://fonts.googleapis.com/"]

}